# penjumlahan
def add(x, y):
  return x + y

result =  add(3, 5)
print(result)

# pengurangan
def minus(x, y):
  return x - y

result = minus (5,3)
print(result)

# perkalian 
def mult(x, y):
  return x * y

result = mult(3, 5)
print(result)

# pembagian
def div(x, y):
  return x / y

result = div(10, 2)
print(result)
